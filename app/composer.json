{
    "require": {
        "symfony/console": "^7.1",
        "phpoffice/phpspreadsheet": "^2.1",
        "morris/lessql": "^1.0",
        "ext-pdo": "*",
        "phroute/phroute": "^2.2",
        "ext-simplexml": "*",
        "spatie/array-to-xml": "^3.3",
        "ext-dom": "*",
        "respect/validation": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
